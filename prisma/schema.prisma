generator client {
  provider = "prisma-client"
  output   = "../src/infrastructure/db/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model demo {
  id          String   @id @default(uuid())
  title       String   @db.VarChar(255)
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
